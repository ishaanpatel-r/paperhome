                 //masonry
                (function ($) {

                    var $container = $('.masonry-container');
                    $container.imagesLoaded(function () {
                        $container.masonry({
                            columnWidth: '.item',
                            itemSelector: '.item',
                            originLeft: true
                        });
                           
                      

                        $container.on('click', '.enlargewrite', function () {
                            $( this ).siblings().removeClass('item--gigante');
                            // change size of item via class
                            $(this).toggleClass('item--gigante');
                            // trigger layout
                           
                            $container.masonry({
                                columnWidth: '.item',
                                itemSelector: '.item',
                                originLeft: true
                            });
                        });

                       
                    });


                 
                    $('a[data-toggle=tab]').each(function () {
                        var $this = $(this);

                        $this.on('shown.bs.tab', function () {

                            $container.imagesLoaded(function () {
                                $container.masonry({
                                    columnWidth: '.item',
                                    itemSelector: '.item',
                                    originLeft: true
                                });

                                $container.on('click', '.enlargewrite', function () {
                                    $( this ).siblings().removeClass('item--gigante');
                                    // change size of item via class
                                    $(this).toggleClass('item--gigante');
                                    // trigger layout
                           
                                    $container.masonry({
                                        columnWidth: '.item',
                                        itemSelector: '.item'
                                    });
                                });

                            });

                        }); 
                    }); 

                  

                })(jQuery);

           
            });
